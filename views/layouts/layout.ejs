<body id="page-top">
    <div id="topbar">
        <%- include('../partials/topnav'); %>
    </div>
    <div id="wrapper">
        <div id="menunav" >
            <%- include('../partials/menunav'); %>
        </div>
        <div id="content-wrapper">
            <div id="body-wr" class="container-fluid">
                <%- body %>
            </div>
            <div id="footer-wr">
                <%- include('../partials/footer'); %>
