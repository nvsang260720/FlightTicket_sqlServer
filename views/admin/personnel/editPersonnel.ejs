<div>
    <div class="container-fluid">
        <h2>Thêm nhân viên</h2>
    </div>
    <section>
        <div class="row">
            <div class="col-md-12">
                <div class="card card-outline card-info">
                    <form action="/admin/edit-personnel" method="POST">
                        <div class="card-body">
                            <% personnel.forEach( per=> { %>
                                <div class="form-group">
                                    <div class="inputChoose">
                                        <label for="exampleInputName">Mã Nhân Viên</label>
                                        <input value="<%= per.MaNhanVien %>" readonly type="text" name="maso"
                                            class="form-control" id="exampleInputPassword1"
                                            placeholder="Nhập tên sản phẩm">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="inputChoose">
                                        <label for="exampleInputName">Mã Chi Nhánh</label>
                                        <select name="machinhanh" class="form-select form-select-lg mb-3"
                                            aria-label=".form-select-lg example">
                                            <option selected>
                                                <%= per.MaChiNhanh %>
                                            </option>
                                            <option value="DN02">DN02</option>
                                            <option value="SG03">SG03</option>
                                            <option value="HN01">HN01</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="inputChoose">
                                        <label>Họ Tên</label>
                                        <input value="<%= per.Ten %>" type="text" name="name" class="form-control"
                                            id="exampleInputPassword1" placeholder="Nguyen van a">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="inputChoose">
                                        <label>Địa Chỉ</label>
                                        <input value="<%= per.DiaChi %>" type="text" name="diachi" class="form-control"
                                            placeholder="05 Nguyen van a">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="inputChoose">
                                        <label>Số Điện Thoại</label>
                                        <input value="<%= per.SoDienThoai %>" type="number" name="moblie"
                                            class="form-control" id="exampleInputPassword1" placeholder="price">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="inputChoose">
                                        <label>Lương</label>
                                        <input value="<%= per.Luong %>" type="number" name="price" class="form-control"
                                            id="exampleInputPassword1" placeholder="price">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <button type="submit" class="btSave">Update</button>
                                </div>
                        </div>

                        <% }) %>
                    </form>
                </div>
            </div>
        </div>
    </section>
</div>